{
    "parser": "@typescript-eslint/parser",
    "extends": ["airbnb-typescript/base"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "rules": {
        "no-console": "off",
        "no-await-in-loop": "off",
        "no-param-reassign": "warn",
        "no-restricted-syntax": "warn"
    }
}
